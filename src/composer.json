{
    "type": "project",
    "license": "proprietary",
    "require": {
        "php": "^8.1",
        "ext-iconv": "*",
        "ext-json": "*",
        "ext-mongodb": "*",
        "alcaeus/mongo-php-adapter": "^1.1",
        "doctrine/mongodb-odm-bundle": "^4.2",
        "guzzlehttp/guzzle": "^7.2",
        "symfony/console": "^5.4",
        "symfony/flex": "^2.1",
        "symfony/framework-bundle": "^5.4",
        "symfony/monolog-bundle": "^3.4",
        "symfony/property-access": "^5.4",
        "symfony/runtime": "5.4.*",
        "symfony/serializer": "^5.4",
        "symfony/twig-bundle": "^5.4",
        "symfony/webpack-encore-bundle": "^1.8",
        "symfony/yaml": "^5.4"
    },
    "require-dev": {
        "doctrine/doctrine-fixtures-bundle": "^3.4",
        "liip/test-fixtures-bundle": "^2.2.1",
        "symfony/browser-kit": "^5.4",
        "symfony/dotenv": "^5.4",
        "symfony/phpunit-bridge": "^6.0",
        "symfony/stopwatch": "^5.4",
        "symfony/web-profiler-bundle": "^5.4"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true,
        "platform": {
            "ext-mongo": "1.6.16"
        },
        "allow-plugins": {
            "symfony/flex": true,
            "symfony/runtime": true
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "npm install": "script",
            "local-php-security-checker": "script",
            "if [$COMPOSER_DEV_MODE -eq 0] \n then \n npm run build \n else \n npm run dev \n fi": "script",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "id": "01C8E3HM874B7GWDC5CG4030XN",
            "allow-contrib": "true",
            "require": "5.4.*"
        }
    }
}
